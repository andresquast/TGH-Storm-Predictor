{
  "metadata": {
    "dataset_name": "Tampa Bay Hurricane Bridge Closure Dataset",
    "description": "Historical hurricane and tropical storm data affecting Tampa Bay area bridges, specifically for Tampa General Hospital bridge closure prediction modeling",
    "created_date": "2024-12-17",
    "last_updated": "2024-12-18",
    "version": "3.0",
    "data_sources": [
      "NOAA National Hurricane Center",
      "Florida Highway Patrol",
      "Tampa Bay Times",
      "Florida Department of Transportation",
      "National Weather Service Tampa Bay",
      "USGS Flood Reports",
      "NHC Tropical Cyclone Reports"
    ],
    "notes": "Bridge closure times are approximate based on news reports and official statements. Some older storms (1996-2016) have estimated closure times based on news archives and historical records. Forward speed calculated from NHC reports. Dataset expanded from 12 to 18 storms with bridge impacts."
  },
