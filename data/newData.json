  "data_dictionary": {
    "name": "Storm name",
    "date": "Date of closest approach or landfall (ISO format)",
    "year": "Year of event",
    "month": "Month of event (1-12)",
    "category_at_tampa": "Hurricane category (Saffir-Simpson scale) when affecting Tampa Bay area, 0=tropical depression, 1=tropical storm",
    "max_wind_tampa": "Maximum sustained winds in mph affecting Tampa Bay area",
    "storm_surge_tampa": "Storm surge in feet measured or estimated for Tampa Bay",
    "track_distance_miles": "Distance in miles from storm center to Tampa General Hospital at closest approach",
    "forward_speed_mph": "Forward speed of storm in mph",
    "closure_hours": "Total hours bridges were closed (null if no closure)",
    "bridge_closed_time": "Approximate time bridges were closed (ISO format)",
    "bridge_opened_time": "Approximate time bridges were reopened (ISO format)",
    "bridges_affected": "List of bridges that were closed",
    "landfall_location": "Where storm made landfall",
    "notes": "Additional context",
    "data_quality": "high/medium/low - quality of closure data"
  },
  "bridge_info": {
    "Sunshine Skyway": {
